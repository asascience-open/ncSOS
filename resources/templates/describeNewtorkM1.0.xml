<sml:SensorML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.opengis.net/sensorML/1.0.1 http://schemas.opengis.net/sensorML/1.0.1/sensorML.xsd"
 xmlns:xlink="http://www.w3.org/1999/xlink"
 xmlns:gml="http://www.opengis.net/gml"
 xmlns:swe="http://www.opengis.net/swe/1.0.1"
 xmlns:sml="http://www.opengis.net/sensorML/1.0.1"
 version="1.0.1">
  <sml:member>
    <sml:System>
      <gml:description />
      <gml:name />
      <gml:boundedBy />
      <sml:identification>
        <sml:IdentifierList />
      </sml:identification>
      <sml:classification>
        <sml:ClassifierList />
      </sml:classification>
      <sml:capabilities />
      <sml:contact>
        <sml:ContactList />
      </sml:contact>
      <sml:history />
      <sml:documentation />
      <sml:components>
        <sml:ComponentList />
      </sml:components>
    </sml:System>
  </sml:member>
</sml:SensorML>
